{
  "name": "exec-path-action",
  "version": "0.0.0",
  "description": "GitHub JavaScript action to determine execution path.",
  "main": "index.js",
  "scripts": {
    "build": "ncc build src/index.js -o dist",
    "release": "semantic-release"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@vercel/ncc": "^0.38.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^24.2.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "extends": "./scripts/plugins/release.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subhamay-bhattacharyya-gha/exec-path-action.git"
  },
  "author": "Subhamay Bhattacharyya",
  "license": "MIT",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/subhamay-bhattacharyya-gha/exec-path-action/issues"
  },
  "homepage": "https://github.com/subhamay-bhattacharyya-gha/exec-path-action#readme"
}
